generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id         Int      @id @default(autoincrement())
  task       String
  Status     Boolean
  created_at DateTime @default(now())

  @@map("tasks")
}

model List {
  id Int @id @default(autoincrement())
  name String @unique

  @@map("lists")
}
